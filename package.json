{
  "name": "edu_meet_admin",
  "version": "1.0.0",
  "description": "Admin panel for managing slots and appointments in the Edu Meet application.",
  "main": "index.js",
  "scripts": {
    "build": "npm run copy-dependencies",
    "add_static_dir": "mkdir -p edu_meet_admin_panel/static",
    "copy-dependencies": "npm run copy-jquery && npm run copy-jquery-ui",
    "copy-jquery": "rsync -av node_modules/jquery edu_meet_admin_panel/static/",
    "copy-jquery-ui": "rsync -av node_modules/jquery-ui edu_meet_admin_panel/static/"
  },
  "keywords": ["django", "admin", "scheduler", "jquery", "jquery-ui"],
  "author": "Asgef <asgefes1@@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "jquery": "^3.7.1",
    "jquery-ui": "^1.14.1"
  }
}
